rjump = (jump self f args) -> fjump jump self f (list (reversed args))
fjump = (jump self f args) ->
  (a, b), _, _, _ = unpack f args 2 2
  jmp = self.loadop jump a delta: 0
  self.load b
  call jmp

PREFIX !! 'or'  = bind fjump 'JUMP_IF_TRUE_OR_POP'
PREFIX !! 'and' = bind fjump 'JUMP_IF_FALSE_OR_POP'
PREFIX !! 'if'  = bind rjump 'JUMP_IF_FALSE_OR_POP'
